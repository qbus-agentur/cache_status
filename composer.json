{
    "name": "qbus/cache-status",
    "type": "typo3-cms-extension",
    "description": "Adds a X-TYPO3-Cache header to indicate cache hits/misses",
    "keywords": [
        "TYPO3",
        "extension"
    ],
    "homepage": "https://www.qbus.de",
    "authors": [
        {
            "name": "Benjamin Franzke",
            "email": "bfr@qbus.de",
            "role": "Developer",
            "homepage": "https://www.qbus.de"
        }
    ],
    "license": "GPL-2.0+",
    "require": {
        "typo3/cms-core": "^7.6.0 || ^8.4.0 || ~9.0.0 || ~9.1.0"
    },
    "autoload": {
        "psr-4": {
            "Qbus\\CacheStatus\\": "Classes"
        }
    },
    "replace": {
        "cache_status": "self.version",
        "typo3-ter/cache-status": "self.version"
    }
}
